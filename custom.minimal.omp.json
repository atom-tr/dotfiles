{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [ {
          "background": "#303030",
          "foreground": "#3C873A",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "style": "diamond",
          "template": " {{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "{{ .Meaning }}\u274c ",
          "type": "exit"
        },
        {
          "foreground": "#ff0000",
          "style": "plain",
          "template": "# ",
          "type": "root"
        },
        {
          "type": "python",
          "style": "powerline",
          "foreground": "#ffffff",
          "background": "#303030",
          "powerline_symbol": "\ue0b0",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} "
        },
        {
          "background": "#458588",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "\u2026",
            "folder_separator_icon": " \ue0b1 ",
            "style": "agnoster_short",
            "max_depth": 3
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#98971A",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "cherry_pick_icon": "\u2713 ",
            "commit_icon": "\u25b7 ",
            "fetch_status": true,
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 "
          },
          "style": "powerline",
          "template": " {{ .HEAD }}{{ if and (eq .Ahead 0) (eq .Behind 0) }} \u2261{{end}}{{ if gt .Ahead 0 }} \u2191{{.Ahead}}{{end}}{{ if gt .Behind 0 }} \u2193{{.Behind}}{{end}} {{ if .Working.Changed }}{{ if .Working.Added }}+{{ .Working.Added }} {{end}}{{ if .Working.Modified }}~{{ .Working.Modified }} {{end}}{{ if .Working.Deleted }}-{{ .Working.Deleted }} {{end}}{{ end }}",
          "type": "git"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
